# ---- Supabase ----
# Your Supabase project URL, e.g. https://xxxxxxxxxxxx.supabase.co
VITE_SUPABASE_URL=
# Your Supabase anon (public) key
VITE_SUPABASE_ANON_KEY=
# Keep the Vite dev proxy for Supabase (recommended in dev)
VITE_SUPABASE_USE_PROXY=1

# ---- Mapbox ----
# Your Mapbox public access token (sk: tokens will NOT work in browser)
VITE_MAPBOX_TOKEN=
# Use the Vite dev proxy for Mapbox (optional)
VITE_USE_MAPBOX_PROXY=1

# ---- Lovable (Market) ----
# App externe Lovable (aucune route interne /fleetmarket)
VITE_USE_EXTERNAL_LOVABLE=true
VITE_LOVABLE_URL=
VITE_MARKET_HASH_PATH=#/

# ---- Convoiturage (Lovable) ----
# App externe Lovable (aucune route interne /convoiturage)
VITE_USE_EXTERNAL_CONVOITURAGE=true
VITE_CONVOITURAGE_URL=
VITE_CONVOITURAGE_HASH_PATH=#/
